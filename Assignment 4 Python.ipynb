    "#11) \n",
    "def fact(n):\n",
    "    if n == 0 or n == 1:\n",
    "        return 1\n",
    "    else:\n",
    "        return n * fact(n-1)\n",
    "    \n",
    "no = int(input(\"Enter the number: \"))\n",
    "\n",
    "if no < 0:\n",
    "    print(\"Not Defined\")\n",
    "else:\n",
    "    result = fact(no)\n",
    "    print(\"The factorial of\", no, \"is\", result)"
   ]
